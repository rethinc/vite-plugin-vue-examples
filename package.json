{
  "name": "vite-plugin-vue-examples-workspaces",
  "description": "Vite plugin to test vue files isolated",
  "author": "re:thinc",
  "license": "GPL-3.0",
  "private": false,
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/rethinc/vite-plugin-vue-examples.git"
  },
  "type": "module",
  "workspaces": [
    "package"
  ],
  "scripts": {
    "dev": "npm run dev -w package",
    "build": "npm run build -w package",
    "lint": "eslint .",
    "fix-lint-errors": "eslint . --fix",
    "check-types": "npm run check-types -w package",
    "verify": "npm run lint && npm run check-types && npm run test",
    "test": "npm run test -w package"
  },
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-vue": "^9.25.0",
    "npm-check-updates": "^16.14.20",
    "prettier": "^3.2.5",
    "typescript-eslint": "^7.7.1"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/vite-plugin-vue-examples.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vite-plugin-vue-examples.js"
    }
  }
}
