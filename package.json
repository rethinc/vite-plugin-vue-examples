{
  "name": "vite-plugin-vue-examples",
  "description": "Vite plugin to test vue files isolated",
  "author": "re:thinc",
  "license": "GPL-3.0",
  "private": false,
  "version": "0.0.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/rethinc/vite-plugin-vue-examples.git"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "fix": "eslint . --fix",
    "lint": "eslint .",
    "check-types": "npx vue-tsc --noEmit",
    "verify": "npm run lint && npm run check-types && npm run test",
    "test": "vitest run"
  },
  "dependencies": {
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-vue": "^9.25.0",
    "npm-check-updates": "^16.14.20",
    "prettier": "^3.2.5",
    "rollup-plugin-copy": "^3.5.0",
    "sass": "^1.75.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.1",
    "vite": "^5.2.10",
    "vite-plugin-dts": "^3.9.0",
    "vitest": "^1.5.2",
    "vue-tsc": "^2.0.14"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/vite-plugin-vue-examples.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vite-plugin-vue-examples.js"
    }
  }
}
