    "check-types": "npx vue-tsc --noEmit",
    "test": "vitest run"
  },
  "devDependencies": {
    "sass": "^1.76.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.0",
    "vitest": "^1.5.3",
    "vue-tsc": "^2.0.16"
  },
  "dependencies": {
    "vue": "^3.4.26",
    "vue-router": "^4.3.2"
  }
}
