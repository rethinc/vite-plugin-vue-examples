{
  "name": "vite-plugin-vue-examples",
  "description": "Vite plugin to test vue files isolated",
  "author": "re:thinc",
  "license": "LGPL-3.0-or-later",
  "version": "1.0.1",
  "type": "module",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rethinc/vite-plugin-vue-examples.git",
    "directory": "package"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/vite-plugin-vue-examples.js"
    }
  },
  "types": "./dist/vite-plugin-vue-examples.d.ts",
  "scripts": {
    "dev": "rm -rf dist && vite build -w -c vite.config.dev.ts",
    "build": "vite build",
    "check-types": "npx vue-tsc --noEmit",
    "test": "vitest run"
  },
  "devDependencies": {
    "@types/node": "^20.12.8",
    "@vitejs/plugin-vue": "^5.0.4",
    "rollup-plugin-copy": "^3.5.0",
    "sass": "^1.76.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.0",
    "vitest": "^1.5.3",
    "vue-tsc": "^2.0.16"
  },
  "dependencies": {
    "vue": "^3.4.26",
    "vue-router": "^4.3.2"
  }
}
